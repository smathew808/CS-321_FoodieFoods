			"\t<head>",
			"\t\t<meta charset=\"utf-8\" name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"\t\t<title>${2:Enter page title}</title>",
			"\t\t<link href=\"${3:Enter css path}\" rel=\"stylesheet\">",
			"\t</head>",
			"",
			"\t<body id=\"page-${4:Enter name}\">",
			"\t\t<div id=\"container\">",
			"\t\t\t<header>",
			"\t\t\t\t<div><img src=\"\" alt=\"\"><span></span></div>",
			"\t\t\t\t<nav>",
