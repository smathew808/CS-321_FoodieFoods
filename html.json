	"table template": {
		"prefix": "table",
		"body": [
			"<table>",
			"\t<caption>${cap:Enter caption}</caption>",
			"\t<thead>",
			"\t\t<tr>",
			"\t\t\t<th>${h1:header}</th>",
			"\t\t\t<th>${h2:header}</th>",
			"\t\t</tr>",
			"\t</thead>",
			"",
			"\t<tfoot>",
			"\t\t<tr>",
			"\t\t\t<td>${f1:footer}</td>",
			"\t\t\t<td>${f2:footer}</td>",
			"\t\t</tr>",
			"\t</tfoot>",
			"",
			"\t<tbody>",
			"\t\t<tr>",
			"\t\t\t<td>${d1:data}</td>",
			"\t\t\t<td>${d2:data}</td>",
			"\t\t</tr>",
			"\t</tbody>",
			"</table>"
		]
	}
